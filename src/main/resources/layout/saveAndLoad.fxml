<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/11"
      fx:controller="com.treasure.hunt.view.widget.SaveAndLoadController">
    <children>
        <Label text="For instances"/>
        <VBox alignment="TOP_CENTER" spacing="10.0">
            <children>
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#save" text="Save current run to file"/>
                <Button mnemonicParsing="false" onAction="#load" text="Load run from file"/>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
