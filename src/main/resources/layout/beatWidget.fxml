<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="com.treasure.hunt.view.widget.BeatWidgetController">
   <children>
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#play" text="Play"/>
            <Button mnemonicParsing="false" onAction="#stop" text="Stop"/>
         </children>
      </HBox>
      <Spinner fx:id="timeBetweenMovesSpinner"/>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
</VBox>
